    <div class="container" data-aos="zoom-in" id="contact">
      <h1>Contact Us</h1>

      <div class="wrapper animated bounceInLeft">
        <div class="company-info">
          <h1 id="main1">ACH Trucking</h1>
        </div>
        <div class="contact">
          <h3 id="two">Email Us</h3>
          {{msg}}
          {{!-- POST ROUTE --}}
          <form method="POST" action="send">
            <p>
              <label>Name</label>
              <input type="text" name="name">
            </p>

            <p>
              <label>Email Address</label>
              <input type="email" name="email">
            </p>
            <p>
              <label>Phone Number</label>
              <input type="text" name="phone">
            </p>
            <div class="form-group">
              <label for="exampleFormControlSelect1">Subject</label>
              <select class="form-control" name="exampleFormControlSelect1">
                <option>Driver Application</option>
                <option>General Inquiry</option>
                <option>Other</option>
              </select>
            </div>
            <p class="full">
              <label>Message / Tell us about yourself</label>
              <textarea name="message" rows="5"></textarea>
            </p>
            <p class="full">
              <button type="submit" data-aos="fade-right" data-aos-duration="6000">Submit</button>
            </p>
          </form>
        </div>
      </div>

